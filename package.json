{
  "name": "pokedex-assistant",
  "version": "1.0.0",
  "description": "Pokedex Assistant - AI-powered Pokemon encyclopedia web app",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "server": "node src/server.js",
    "web-server": "node src/web-server.js",
    "dev": "node --watch src/index.js",
    "test": "node src/test.js",
    "web": "vite",
    "web:dev": "vite --host",
    "web:build": "vite build",
    "web:preview": "vite preview",
    "full:dev": "concurrently \"npm run web-server\" \"npm run web:dev\"",
    "build": "npm run web:build",
    "preview": "npm run web:preview",
    "generate-icons": "node scripts/create-simple-icons.js && node scripts/convert-svg-to-png.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "canvas": "^3.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.32",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  },
  "keywords": [
    "pokemon",
    "pokedex",
    "react",
    "web-app",
    "ai",
    "chatbot"
  ],
  "author": "",
  "license": "MIT"
}
